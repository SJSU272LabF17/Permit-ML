<html>
<head>
    <title>Login</title>
    <link rel="stylesheet"
          href="./css/login.css">
    <link rel="stylesheet"
          href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <script
            src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>



    <header id="header">
        <div class="container">




            </div>
        </div>
    </header>

</head>
<body>
<div class="container-fluid">

    <div class="row">
        <div
                class="loginscreen"
                style="margin-top: 10%;">
            <div id="panel"class="panel panel-primary">
                <div id="panel1"class="panel-heading">Login</div>
                <div class="panel-body">
                    <form class="form-horizontal" role="form" method="post"
                          action="afterLogin">
                        <div class="form-group">
                            <label class="col-sm-4 col-md-4 col-lg-4">Username</label>
                            <div class="col-sm-8 col-md-8 col-lg-8">
                                <input type="email" class="form-control" name="username"
                                       id="username" placeholder="Email Id">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-4 col-md-4 col-lg-4" for="password">Password</label>
                            <div class="col-sm-8 col-md-8 col-lg-8">
                                <input type="password" class="form-control"
                                       name="password" id="password" placeholder="Password">

                            </div>
                        </div>
                        <div class="form-group">
                            <div
                                    class="col-xs-offset-4 col-sm-offset-4 col-md-offset-4 col-lg-offset-4">
                                <button type="submit" class="btn btn-primary">Login</button>



                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
</body>
</html>